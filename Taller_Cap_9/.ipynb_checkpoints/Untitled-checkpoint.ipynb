    "x=np.arange(2,16,0.1)\n",
    "y=(2*x)-6\n",
    "z=(-6*x)+34\n",
    "ptl.plot(x,y,'-g',label=\"Y=2X-6\")\n",
    "ptl.plot(x,z,'-y',label=\"Z=-6X+34\")\n",
    "ptl.legend(loc=\"upper left\")\n",
    "ptl.grid(True)\n",
    "ptl.show()"
